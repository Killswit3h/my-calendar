{
    "name": "my-cal-starter",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "node scripts/prebuild.js && prisma generate && npm --prefix my-calendar-main exec prisma generate && next build",
        "build:edge": "prisma migrate deploy || echo skip-migrate && prisma generate --no-engine && npm --prefix my-calendar-main exec prisma generate && npx --yes @cloudflare/next-on-pages@1.13.16",
        "start": "next start",
        "postinstall": "prisma generate && npm --prefix my-calendar-main exec prisma generate",
        "generate:edge": "prisma generate --no-engine",
        "generate:node": "prisma generate",
        "test": "vitest",
        "check:design": "node scripts/check-design.mjs",
        "cf:build": "npm run build:edge",
        "pdf:job-by-job": "tsx scripts/render-job-by-job.ts",
        "pdf:daily": "tsx src/scripts/generate-daily-table-pdf.ts",
        "db:whoami": "tsx scripts/db-whoami.ts",
        "report:probe": "tsx scripts/report-probe.ts",
        "test:e2e": "playwright test"
    },
    "dependencies": {
