# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/my_calendar"
DIRECT_URL="postgresql://username:password@localhost:5432/my_calendar"

# Prisma Accelerate (for edge runtime)
PRISMA_ACCELERATE_URL="prisma://accelerate.prisma-data.net/?api_key=your_api_key"

# Application Environment
NODE_ENV="development"
NEXT_RUNTIME="nodejs"

# Testing
PLAYWRIGHT_TEST="0"

# Optional: AWS S3 Configuration (for file uploads)
AWS_ACCESS_KEY_ID="your_access_key"
AWS_SECRET_ACCESS_KEY="your_secret_key"
AWS_REGION="us-east-1"
AWS_S3_BUCKET="your_bucket_name"

# Optional: Vercel Blob Storage
BLOB_READ_WRITE_TOKEN="your_blob_token"

# Web Push Notifications
WEB_PUSH_PUBLIC_KEY="replace-with-generated-public-key"
WEB_PUSH_PRIVATE_KEY="replace-with-generated-private-key"
WEB_PUSH_CONTACT="mailto:notifications@example.com"
NEXT_PUBLIC_WEB_PUSH_PUBLIC_KEY="replace-with-generated-public-key"
